# grape-api alert related messages
GRAPE_API_ALERTS_METRICS \[%{GREEDYDATA:timestamp}\] %{LOGLEVEL:loglevel} -- \[alert-metrics\] type=%{GREEDYDATA:message_type} failed=%{NUMBER:failed_alerts:int} sent=%{NUMBER:sent_alerts:int} not_sent=%{NUMBER:not_sent_alerts:int}
GRAPE_API_ALERTS_PROCESS \[%{GREEDYDATA:timestamp}\] %{LOGLEVEL:loglevel} -- processed %{NUMBER:alerts_processed:int} alerts in %{NUMBER:alerts_processing_time:float} seconds
GRAPE_API_ALERTS_MAIL_SENT \[%{GREEDYDATA:timestamp}\] %{LOGLEVEL:loglevel} -- Mail sent for alert '%{GREEDYDATA:alert_title}' \(#%{NUMBER:alert_id:int}\)
GRAPE_API_ALERTS_ALERT \[%{GREEDYDATA:timestamp}\] %{LOGLEVEL:loglevel} -- Alert: '%{GREEDYDATA:alert_title}' \(#%{NUMBER:alert_id:int}\), to <%{GREEDYDATA:alert_user_name} <%{GREEDYDATA:alert_user_mail}>>
GRAPE_API_ALERTS_NO_REPORTS \[%{GREEDYDATA:timestamp}\] %{LOGLEVEL:loglevel} -- No reports for '%{GREEDYDATA:alert_title}' \(#%{NUMBER:alert_id:int}\)
GRAPE_API_ALERTS_GENERIC \[%{GREEDYDATA:timestamp}\] %{LOGLEVEL:loglevel} -- %{GREEDYDATA:message_text}
GRAPE_API_ALERTS_DEPRECATION \[DEPRECATION\] %{GREEDYDATA:deprecation_message}
GRAPE_API_ALERTS %{GRAPE_API_ALERTS_METRICS}|%{GRAPE_API_ALERTS_PROCESS}|%{GRAPE_API_ALERTS_DEPRECATION}|%{GRAPE_API_ALERTS_MAIL_SENT}|%{GRAPE_API_ALERTS_ALERT}|%{GRAPE_API_ALERTS_NO_REPORTS}|%{GRAPE_API_ALERTS_GENERIC}