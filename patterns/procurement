RAILS_TIMESTAMP %{YEAR}-%{MONTHNUM}-%{MONTHDAY} %{HOUR}:%{MINUTE}:%{SECOND} %{ISO8601_TIMEZONE}
RAILS_CONTROLLER %{RUBY_CLASS}#%{METHOD_NAME:method_name}
RAILS_HEADER Started %{HTTP_METHOD:method} "%{URIPATHPARAM:url}" for %{IPORHOST:client} at %{RAILS_TIMESTAMP:request_timestamp}
RAILS_PROCESSING_AS_PARAMATERS HTML   Parameters: %{RUBY_HASH:parameters}
RAILS_PROCESSING_AS_SESSION HTML Session authentication: %{URI}
RAILS_PROCESSING_AS_GENERIC \*/\*
RAILS_PROCESSING_AS (%{RAILS_PROCESSING_AS_GENERIC}|%{RAILS_PROCESSING_AS_SESSION}|%{RAILS_PROCESSING_AS_PARAMATERS})
RAILS_PROCESSING \W*Processing by %{RAILS_CONTROLLER} as %{RAILS_PROCESSING_AS}
RAILS_SESSION_COOKIE \W*generate_session_cookie\W*%{SESSID:session_id}
RAILS_SESSION_NOT_AUTHENTICATED \W*NOT AUTHENTICATED
RAILS_SESSION_AUTHENTICATED \W*AUTHENTICATED: session_permission: {"user_id"=>%{NUMBER:user_id:int}, "dfa_id"=>%{NUMBER:dfa_id}, "first_name"=>"(?<first_name>[^"]*)", "last_name"=>"(?<last_name>[^"]*)", "user_packages"=>%{RUBY_ARRAY:user_packages}} ON SESSION: session\[:current_user\]: %{NUMBER}
RAILS_SESSION %{RAILS_SESSION_COOKIE}?(%{RAILS_SESSION_NOT_AUTHENTICATED}|%{RAILS_SESSION_AUTHENTICATED}|)
RAILS_RENDER \W*Rendered %{RELATIVE_PATH}( within %{RELATIVE_PATH})? \(%{NUMBER}ms\)
RAILS_RENDERS %{RAILS_RENDER}*
RAILS_EXCEPTION_FIRST ([^\(]*\(.*\))
RAILS_EXCEPTION \W*(?<exception>%{RAILS_EXCEPTION_FIRST}:)(?<rest>.*)
RAILS_COMPLETED \W*Completed %{NUMBER:response_code} %{HTTP_RESPONSE_CODES:response_status} in %{NUMBER:complete_time:float}ms (\(Views: %{NUMBER:render_time:float}ms\)|%{RAILS_EXCEPTION})
RAILS_SERVED \W*Served asset %{UNIXPATH:asset} - %{NUMBER:response_code} %{HTTP_RESPONSE_CODES:response_status} \(%{NUMBER:complete_time:float}ms\)
RAILS_FOOTER (%{RAILS_COMPLETED}|%{RAILS_SERVED})
RAILS_LOG %{RAILS_HEADER}(%{RAILS_PROCESSING}%{RAILS_SESSION}%{RAILS_RENDERS}|)%{RAILS_FOOTER}